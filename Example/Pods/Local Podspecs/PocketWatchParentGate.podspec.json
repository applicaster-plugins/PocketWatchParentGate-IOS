{
  "name": "PocketWatchParentGate",
  "version": "0.1.0",
  "summary": "A PocketWatchParentGate plugin that integrates to Applicaster Zapp based applications",
  "description": "A PocketWatchParentGate plugin that integrates to Applicaster Zapp based applications",
  "homepage": "https://github.com/applicaster-plugins/PocketWatchParentGate-IOS",
  "license": "MIT",
  "authors": {
    "Andrii Novoselskyi": "andrii.novoselskyi@corewillsoft.com"
  },
  "source": {
    "git": "git@github.com:applicaster-plugins/PocketWatchParentGate-IOS.git",
    "tag": "0.1.0"
  },
  "platforms": {
    "ios": "10.0"
  },
  "requires_arc": true,
  "frameworks": "UIKit",
  "source_files": "PocketWatchParentGate/*",
  "resources": "PocketWatchParentGate/Assets/*",
  "xcconfig": {
    "CLANG_ALLOW_NON_MODULAR_INCLUDES_IN_FRAMEWORK_MODULES": "YES",
    "ENABLE_BITCODE": "YES",
    "OTHER_LDFLAGS": "$(inherited)",
    "FRAMEWORK_SEARCH_PATHS": "$(inherited) \"${PODS_ROOT}\"/**",
    "LIBRARY_SEARCH_PATHS": "$(inherited) \"${PODS_ROOT}\"/**",
    "SWIFT_VERSION": "5.1"
  },
  "dependencies": {
    "ZappPlugins": [

    ]
  },
  "subspecs": [
    {
      "name": "Classes",
      "source_files": "PocketWatchParentGate/Classes/*",
      "subspecs": [
        {
          "name": "Popups",
          "source_files": "PocketWatchParentGate/Classes/Popups/*"
        }
      ]
    },
    {
      "name": "Extensions",
      "source_files": "PocketWatchParentGate/Extensions/*"
    }
  ]
}
